{
  "tabWidth": 4,
  "useTabs": false,
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-tailwindcss","@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
    "^react$",                        
    "^react-.*",                      
    "^next(/.*)?$",                   
    "^@/lib/(.*)$",                  
    "^@/components/(.*)$",           
    "<THIRD_PARTY_MODULES>",           
    "^[./]"                        
  ]
}
