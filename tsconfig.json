{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@/db/*": ["./src/lib/db/*"]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    },
    "exclude": [
        "dist",
        "src/components/ui",
        "adapters--astrojs-node-8.3.4",
        "utils"
    ]
}
